<!--pages/create-catagory/create-catagory.wxml-->

<view>
  <text class='create-catagory-title'>新建分类</text>
</view>
<view>
  <input class='catagoryName' bindconfirm="inputName" placeholder='分类名'></input>
</view>

<scroll-view class='scroll-view' scroll-y>
<item-select-list bind:checkboxChange="checkboxChange">
</item-select-list>
</scroll-view>

<button class='button' hover-class='button-hover' bindtap='create'>创建</button>