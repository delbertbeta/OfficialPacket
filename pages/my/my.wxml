<!--pages/my/my.wxml-->
<view class='my-container'>
  <view class='info-container'>
    <view class='info-text'>
      <text class='user-name'>{{userInfo.nickName}}</text>
      <text>您好</text>
    </view>
    <image class='info-face' mode="aspectFill" src='{{userInfo.avatarUrl}}'></image>
  </view>
  <view class='function-list'>
    <text bindtap='goToMySubscribe'>我的订阅</text>
    <text bindtap='goToMyCatagory'>我的分类</text>
  </view>
</view>