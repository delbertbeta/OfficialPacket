<!--components/item-select-list/item-select-list.wxml-->

<checkbox-group bindchange="checkboxChange">
  <view wx:for="{{[1,2,3]}}" class='checkbox-item'>
    <image class='official-face' src='/static/face.jpg' mode='aspectFill'></image>
    <view class='info-container'>
      <text class='official-name'>delbertbeta</text>
      <text class='official-desc'>震惊！华工创造301火热开场！</text>
    </view>
    <checkbox class='checkbox' value='{{item}}' />
  </view>
</checkbox-group>